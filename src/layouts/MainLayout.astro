---
import HTML from "~/components/HTML.astro";
import  SpeedInsights  from "@vercel/speed-insights/astro"

type Props = {
  title?: string;
  description?: string;
};

const { title, description } = Astro.props;
---

<HTML {title} {description}>
  <SpeedInsights/>
  <Fragment slot="body">
    <slot />
  </Fragment>
</HTML>
